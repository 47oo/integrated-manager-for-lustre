{
  "name": "chroma_ui",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start"
  },
  "devDependencies": {
    "jasmine-core": "2.4.1",
    "karma": "0.13.19",
    "karma-chrome-launcher": "0.2.2",
    "karma-firefox-launcher": "0.1.7",
    "karma-jasmine": "0.3.6",
    "karma-junit-reporter": "0.3.8",
    "karma-safari-launcher": "0.1.1",
    "karma-script-launcher": "0.1.0"
  },
  "jshintConfig": {
    "predef": [
      "angular",
      "_",
      "module",
      "inject",
      "jasmine",
      "spyOn",
      "it",
      "console",
      "describe",
      "expect",
      "beforeEach",
      "afterEach",
      "mock",
      "Mock",
      "Timecop",
      "waits",
      "waitsFor",
      "runs"
    ],
    "bitwise": true,
    "curly": false,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "indent": 2,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxcomplexity": 6,
    "maxlen": 120,
    "eqnull": true,
    "multistr": true,
    "browser": true,
    "jquery": true
  }
}
