{
  "name": "stub-daddy",
  "author": "Will Johnson",
  "description": "Stub API service used for integration testing.",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "bunyan": "1.3.5",
    "configulator": "1.0.2",
    "jsonschema": "0.4.0",
    "wiretree": "0.2.5",
