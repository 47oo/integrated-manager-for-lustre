    "minijasminenodewrap": "1.0.1",
    "request-then": "0.0.1"
  },
  "main": "index.js",
  "scripts": {
    "test": "mjnw",
    "integration": "mjnw test/integration/*-spec.js",
    "unit": "mjnw test/unit/*-spec.js",
    "cover": "cover",
    "ziplock": "../ziplocker/bin/ziplocker --overrides.ziplockDir='../../../../chroma-externals/'",
    "zipdev": "../ziplocker/bin/zip-install-dev --overrides.ziplockDir='../../../../chroma-externals/'",
    "jenkins": "mjnw --reportType=junit --JUnitReportSavePath=./ --JUnitReportFilePrefix=stub-daddy-results"
  },
  "bin": {
    "stub-daddy": "index.js"
  },
