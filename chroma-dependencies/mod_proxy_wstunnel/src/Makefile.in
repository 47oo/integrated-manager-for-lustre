APXS = @APXS@

DESTDIR =
LIBEXECDIR = @LIBEXECDIR@

CPPFLAGS = @CPPFLAGS@
CFLAGS = @CFLAGS@
LDFLAGS = @LDFLAGS@
LDLIBS = @LDLIBS@

all : mod_proxy_wstunnel.la

mod_proxy_wstunnel.la : mod_proxy_wstunnel.c
	$(APXS) -c $(CPPFLAGS) $(CFLAGS) mod_proxy_wstunnel.c $(LDFLAGS) $(LDLIBS)

$(DESTDIR)$(LIBEXECDIR) :
	mkdir -p $@

install : all $(DESTDIR)$(LIBEXECDIR)
	$(APXS) -i -S LIBEXECDIR=$(DESTDIR)$(LIBEXECDIR) -n 'mod_proxy_wstunnel' mod_proxy_wstunnel.la

clean :
	-rm -rf .libs
	-rm -f mod_proxy_wstunnel.o mod_proxy_wstunnel.la mod_proxy_wstunnel.lo mod_proxy_wstunnel.slo mod_proxy_wstunnel.loT
	-rm -f config.log config.status
	-rm -rf autom4te.cache

distclean : clean
	-rm -f Makefile Makefile.in

realclean : distclean
	-rm -f configure
